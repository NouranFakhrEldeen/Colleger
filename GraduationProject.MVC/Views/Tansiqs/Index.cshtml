@model IEnumerable<GraduationProject.Data.Entities.Tansiq>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Division.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Faculty.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Specialization.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Startgrade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endgrade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actual)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Division.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Specialization.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Startgrade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endgrade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actual)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
